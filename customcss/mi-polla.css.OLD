    table.tabla_np { 
      border-collapse: separate; 
      text-align: left; 
      width: 100%;
      background-color: var(--azulRusia);
      border-top-left-radius: 0.2em;
      border-top-right-radius: 0.2em;
    } 
    
    
    
    table.tabla_np tbody{ 
      background-color: var(--blanco);
    }

    table.tabla_np td{
      max-height: 20px !important;

    }

    table.tabla_np td, table.tabla_np th { 
      padding: 1px 2px; 
    }
    table.tabla_np thead th {
      
      color: var(--blanco);
      
      font-size: 0.7em; 
      font-weight: bold;
      text-align: center;
    } 
    table.tabla_np thead th:first-child { 
      border: none; 
    }
    table.tabla_np tbody td { 
      color: rgb(80, 76, 76); 
      border-bottom: 0.2em white solid;
      font-size: 0.8em; 
      font-weight: normal; 
      text-align: center;
      
    }

    table.tabla_np tbody td:first-child { 
      border-left: none; 
    }
    table.tabla_np tbody tr:last-child td { 
      border-bottom: none; 
    }



